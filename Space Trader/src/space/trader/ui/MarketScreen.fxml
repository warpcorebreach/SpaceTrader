<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="space.trader.ui.MarketScreenController">
   <children>
      <Label layoutX="218.0" layoutY="29.0" text="MarketPlace" />
      <Button layoutX="513.0" layoutY="169.0" mnemonicParsing="false" onMouseClicked="#purchase" text="Purchase" />
      <Button layoutX="513.0" layoutY="222.0" mnemonicParsing="false" onMouseClicked="#sell" prefHeight="31.0" prefWidth="80.0" text="Sell" />
      <Label fx:id="label1" layoutX="78.0" layoutY="18.0" text="Label" />
      <Separator layoutX="493.0" layoutY="207.0" prefHeight="7.0" prefWidth="119.0" />
      <TableView fx:id="table" layoutX="27.0" layoutY="69.0" onMouseClicked="#selection" prefHeight="305.0" prefWidth="465.0">
        <columns>
          <TableColumn fx:id="goodCol" editable="false" prefWidth="156.0" text="Goods" />
          <TableColumn fx:id="priceCol" editable="false" prefWidth="156.0" text="Price (Credits)" />
            <TableColumn fx:id="quantCol" editable="false" prefWidth="152.0" text="Quantity" />
        </columns>
      </TableView>
      <Button layoutX="375.0" layoutY="24.0" mnemonicParsing="false" onAction="#EnterPlanetScreen" text="Back To Planet" />
   </children>
</AnchorPane>
